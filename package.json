  "author": "soptorshibiswas",
  "dependencies": {
    "antd": "4.18.2",
    "aws-sdk": "^2.1105.0",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",

    "@types/sharp": "^0.30.0",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
